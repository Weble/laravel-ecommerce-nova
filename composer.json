{
    "name": "weble/laravel-ecommerce-nova",
    "description": "Laravel Ecommerce Nova Tools, Resources and Fields",
    "keywords": [
        "laravel",
        "nova"
    ],
    "homepage": "https://github.com/weble/laravel-ecommerce-nova",
    "license": "MIT",
    "authors": [
        {
            "name": "Weble",
            "email": "daniele@weble.it",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0",
        "weble/laravel-ecommerce": "^3.0",
        "madewithlove/laravel-nova-uuid-support": "^v1.1.1"
    },
    "autoload": {
        "psr-4": {
            "Weble\\LaravelEcommerceNova\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weble\\LaravelEcommerceNova\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weble\\LaravelEcommerceNova\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/weble/laravel-ecommerce"
        }
    ]
}
